<h1 class="text-center"><%= pluralize(@search_tasks.count, "Result") %></h1>
<div class="col-lg-6 mx-auto">
  <%= render "static_pages/priority_task_table", tasks: @search_tasks%>
  <%= will_paginate @search_tasks, renderer: WillPaginate::ActionView::BootstrapLinkRenderer , previous_label: "Previous", 
  next_label: "Next",inner_window: 1, outer_window: 0 %>
  <%= link_to "Back", :back, class: "btn btn-outline-secondary" %>
</div>
